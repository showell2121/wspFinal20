<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Gets bootstrap header file -->
    <%- include("./partials/header.ejs") %>

    <title>Signin</title>
    <style>
        /*Small devices like phones  */
        @media (min-width: 300px) and (max-width: 768px) {

            #img {
                width: 90%;
                height: 100px;
                margin-left: 10%;
                margin-bottom: 5%;

            }            
        }


        /* Medium devices (tablets, less than 992px) */
        @media (min-width: 769px) and (max-width: 991.98px) {
            #img {
                width: 90%;
                height: 100px;
                margin-left: 10%;
                margin-bottom: 5%;

            }
        }

        /* Large devices like laptops */
        @media (min-width:992px) {

            #img {
                width: 100%;
                height: 100px;                
                margin-bottom: 5%;
            }
        }
    </style>
</head>

<body>

    <%- include("./partials/nav.ejs", {page: "class", user, classify: null}) %>



    <img alt="Image result for uco logo&quot;" id="img" src="http://localhost:5000/images/enroll.png" />
    




    <% if(error) { %>

    <div align="center">
        <p style="color:red; "> <%= error %></p>

    </div>
    <% }  %>


        

      


    <!-- Gets bootstrap footer file -->
    <%- include("./partials/footer.ejs") %>
</body>

</html>